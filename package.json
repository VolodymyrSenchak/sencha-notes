    "rules": {
      "jsx-quotes": [
        2,
        "prefer-double"
      ],
      "max-len": [
        "error",
        {
          "code": 120
        }
      ],
      "quotes": [
        2,
        "double"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
